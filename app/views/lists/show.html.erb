<h1><%= @list.name %></h1>
<ol>
  <% @list.tasks.each do |task| %>
    <li><%= link_to task.name, list_task_path(@list, task) %></li>
  <% end %>
</ol>
<%= link_to "Edit List", edit_list_path(@board, @list), class: "ui green button" %>
<%= link_to "Delete List", @list, method: :delete, class: "ui red button" %>